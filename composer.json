{
  "name": "pfcode/symfony-i18n-comparator",
  "description": "Utility for comparison of duplicated translation files in Symfony projects.",
  "minimum-stability": "stable",
  "version": "1.0.10",
  "keywords": [
    "translation",
    "symfony",
    "symfony-bundle",
    "symfony-command"
  ],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "pfcode",
      "email": "mail@pfco.de"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "symfony/config": "^4.4 || ^5.0",
    "symfony/dependency-injection": "^4.4 || ^5.0",
    "symfony/http-kernel": "^4.4 || ^5.0",
    "symfony/yaml": "^4.4 || ^5.0",
    "symfony/console": "^4.4 || ^5.0",
    "symfony/translation": "^4.4 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Pfcode\\I18nComparatorBundle\\": "src"
    }
  }
}
